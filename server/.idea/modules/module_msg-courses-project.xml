<?xml version="1.0" encoding="UTF-8"?>
<project name="module_msg-courses-project" default="compile.module.msg-courses-project">
  <dirname property="module.msg-courses-project.basedir" file="${ant.file.module_msg-courses-project}"/>
  
  <property name="module.jdk.home.msg-courses-project" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.msg-courses-project" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.msg-courses-project" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.msg-courses-project" value="-encoding UTF-8 -source 1.8 -target 1.8 ${compiler.args}"/>
  
  <property name="msg-courses-project.output.dir" value="${module.msg-courses-project.basedir}/../../out/production/msg-courses-project"/>
  <property name="msg-courses-project.testoutput.dir" value="${module.msg-courses-project.basedir}/../../out/test/msg-courses-project"/>
  
  <path id="msg-courses-project.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="msg-courses-project.module.production.classpath">
    <path refid="${module.jdk.classpath.msg-courses-project}"/>
  </path>
  
  <path id="msg-courses-project.runtime.production.module.classpath">
    <pathelement location="${msg-courses-project.output.dir}"/>
  </path>
  
  <path id="msg-courses-project.module.classpath">
    <path refid="${module.jdk.classpath.msg-courses-project}"/>
    <pathelement location="${msg-courses-project.output.dir}"/>
  </path>
  
  <path id="msg-courses-project.runtime.module.classpath">
    <pathelement location="${msg-courses-project.testoutput.dir}"/>
    <pathelement location="${msg-courses-project.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.msg-courses-project">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.msg-courses-project">
    <patternset refid="excluded.from.module.msg-courses-project"/>
  </patternset>
  
  
  <target name="compile.module.msg-courses-project" depends="compile.module.msg-courses-project.production,compile.module.msg-courses-project.tests" description="Compile module msg-courses-project"/>
  
  <target name="compile.module.msg-courses-project.production" depends="register.custom.compilers" description="Compile module msg-courses-project; production classes"/>
  
  <target name="compile.module.msg-courses-project.tests" depends="register.custom.compilers,compile.module.msg-courses-project.production" description="compile module msg-courses-project; test classes" unless="skip.tests"/>
  
  <target name="clean.module.msg-courses-project" description="cleanup module">
    <delete dir="${msg-courses-project.output.dir}"/>
    <delete dir="${msg-courses-project.testoutput.dir}"/>
  </target>
</project>